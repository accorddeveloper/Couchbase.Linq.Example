@model Couchbase.Linq.Example.Models.AirlineModel
@{
    ViewBag.Title = $"Change Tracking - Delete {Model.Name}";
}
<h2>@ViewBag.Title.</h2>

@using (Html.BeginForm())
{
<table class="input">
    <tr>
        <td>@Html.LabelFor(m => m.Name)</td>
        <td>@Html.DisplayFor(m => m.Name)</td>
    </tr>
    <tr>
        <td>@Html.LabelFor(m => m.Callsign)</td>
        <td>@Html.DisplayFor(m => m.Callsign)</td>
    </tr>
    <tr>
        <td></td>
        <td><input type="submit" value="Delete" /></td>
    </tr>
</table>
}

<code>
    @Couchbase.Linq.Example.Logging.RequestQueryAppender.GetLastQuery(Context)
</code>